import os
from google.cloud import storage


os.environ['GOOGLE_APPLICATION_CREDENTIALS'] = 'serviceKey_googleCloud.json'

storage_client = storage.Client()


# Create new bucket #
#bucket = storage_client.bucket("test_bucket_bdiaw7ad")
#bucket.location = "US"
#buk = storage_client.create_bucket(bucket)

# get exisiting bucket #
# bucket = storage_client.get_bucket('buckets_name') #

# send data # 
def upload(blob_name, file_path, bucket_name):
	try:
		bucket = storage_client.get_bucket(bucket_name)
		blob = bucket.blob(blob_name)
		blob.upload_from_filename(file_path)
		return True
	except Exception as e:
		print(e)
		return False
upload('test','testerfile.txt','test_bucket_bdiaw7ad')